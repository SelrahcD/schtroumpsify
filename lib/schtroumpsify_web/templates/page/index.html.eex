<div class="container flex justify-center">

  <ul id="tweets" class="w-4/5">
  <%= for tweet <- @tweets do %>
  	<%= render("tweet.html", tweet: tweet) %>
  <% end %>
  </ul>

</div>

<script id="template-tweet-item" type="text/template">
  <%= render("tweet.html", tweet: "{{tweet}}") %>
</script>
